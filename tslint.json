{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "jsRules": {},
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-console": false
  }
}